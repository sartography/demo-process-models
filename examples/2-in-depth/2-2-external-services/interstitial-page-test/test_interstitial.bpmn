      <bpmn:outgoing>Flow_0dym8fs</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:scriptTask id="task_4" name="A slow script">
      <bpmn:extensionElements>
        <spiffworkflow:instructionsForEndUser>Here we make some complex calculations using calculus, linear algebra so that we might reflect on the complexities of the space-time continuum.  {{my_message}}  
</spiffworkflow:instructionsForEndUser>
